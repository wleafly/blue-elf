    <ScrollView
        android:id="@+id/order_set2_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/background_gray"
        android:scrollbars="none"
        android:paddingTop="20dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginRight="20dp"
            android:layout_marginLeft="20dp"
            android:orientation="vertical">


            <LinearLayout
                android:id="@+id/time_layout"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/layout_border_style_shadow"
                android:elevation="5dp"
                android:layout_margin="5dp"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    android:focusable="true"
                    android:focusableInTouchMode="true"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="3"
                        android:gravity="center"
                        android:textSize="18dp"
                        android:textColor="@color/black"
                        android:text="@string/interval_time">
                    </TextView>
                    <EditText
                        android:id="@+id/interval_time"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@null"
                        android:textCursorDrawable="@drawable/color_cursor"
                        android:layout_weight="1">
                    </EditText>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="1.5dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:background="@color/gray1"></LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    android:focusable="true"
                    android:focusableInTouchMode="true"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="3"
                        android:gravity="center"
                        android:textSize="18dp"
                        android:textColor="@color/black"
                        android:text="@string/test_time">
                    </TextView>
                    <EditText
                        android:id="@+id/test_time"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@null"
                        android:textCursorDrawable="@drawable/color_cursor"
                        android:layout_weight="1">
                    </EditText>

                </LinearLayout>
                
                

            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="50dp"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:gravity="right">
                <Button
                    android:id="@+id/time_btn"
                    android:layout_width="100dp"
                    android:layout_height="50dp"
                    android:textSize="18dp"
                    android:textColor="@color/black"
                    android:text="@string/write_in">

                </Button>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/cleaning_brush"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="gone">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/layout_border_style_shadow"
                    android:elevation="5dp"
                    android:layout_margin="5dp"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:focusable="true"
                        android:focusableInTouchMode="true"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="2"
                            android:gravity="center"
                            android:textSize="18dp"
                            android:textColor="@color/black"
                            android:text="@string/cleaning_time">
                        </TextView>
                        <EditText
                            android:id="@+id/cleaning_time"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@null"
                            android:textCursorDrawable="@drawable/color_cursor"
                            android:layout_weight="1">
                        </EditText>

                    </LinearLayout>



                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="50dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    android:gravity="right">
                    <Button
                        android:id="@+id/cleaning_time_btn"
                        android:layout_width="100dp"
                        android:layout_height="50dp"
                        android:textSize="18dp"
                        android:textColor="@color/black"
                        android:text="@string/write_in">

                    </Button>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/layout_border_style_shadow"
                    android:elevation="5dp"
                    android:layout_margin="5dp"
                    android:orientation="vertical">


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:focusable="true"
                        android:focusableInTouchMode="true"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="2"
                            android:gravity="center"
                            android:textSize="18dp"
                            android:textColor="@color/black"
                            android:text="@string/cleaning_cycles">
                        </TextView>
                        <EditText
                            android:id="@+id/cycles"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@null"
                            android:textCursorDrawable="@drawable/color_cursor"
                            android:layout_weight="1">
                        </EditText>

                    </LinearLayout>



                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="50dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    android:gravity="right">
                    <Button
                        android:id="@+id/cycles_btn"
                        android:layout_width="100dp"
                        android:layout_height="50dp"
                        android:textSize="18dp"
                        android:textColor="@color/black"
                        android:text="@string/write_in">

                    </Button>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:gravity="center">

                    <Button
                        android:id="@+id/reset_btn"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:elevation="2dp"
                        android:text="@string/reset_cleaning_brush"
                        android:textSize="20dp">

                    </Button>
                </LinearLayout>

            </LinearLayout>





        </LinearLayout>

    </ScrollView>



</LinearLayout>